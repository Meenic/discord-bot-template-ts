{
  "name": "discord-bot-template-ts",
  "version": "1.0.1",
  "description": "A customizable TypeScript template for creating Discord bots.",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc --build",
    "start": "node dist/main.js",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "lint": "eslint .",
    "test": "jest",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Meenic/discord-bot-template-ts.git"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript"
  ],
  "author": "Meenic",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "discord.js": "^14.16.2",
    "dotenv": "^16.4.5",
    "mysql2": "^3.11.3",
    "prisma": "^5.21.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^22.6.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
